[linux]
${LINUX_HOST_IP} ansible_user=${LINUX_HOST_USER} ansible_ssh_private_key_file=${LINUX_PRIVATE_KEY}

[windows]
${WINDOWS_HOST_IP}

[windows:vars]
ansible_user=${WINDOWS_HOST_USER}
ansible_password=${WINDOWS_HOST_PASSWORD}
ansible_connection=winrm
ansible_winrm_server_cert_validation=ignore
ansible_port=5986
ansible_winrm_scheme=https
ansible_winrm_transport=basic